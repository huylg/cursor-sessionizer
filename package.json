{
  "name": "cursor-sessionizer",
  "displayName": "VS Code Sessionizer",
  "description": "Fuzzy find and open directories",
  "publisher": "omercohen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/huy.ly/cursor-sessionizer"
  },
  "version": "1.0.9",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sessionizer.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sessionizer.open",
        "title": "Sessionizer: Open"
      }
    ],
    "configuration": {
      "title": "VS Code Sessionizer",
      "properties": {
        "sessionizer.sessionRoots": {
          "description": "The root directories of sessions, for example: [\"~/Desktop/projects\", \"~/Desktop/work\", \"/Users/workspace/*\"]",
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.26.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "glob": "^11.0.3"
  }
}
